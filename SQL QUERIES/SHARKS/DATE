CREATE TABLE IF NOT EXISTS TABELA (
    id INT(11) AUTO_INCREMENT PRIMARY KEY,
    DATE DATE,
    YEAR INT(4),
    TYPE VARCHAR(50),
    COUNTRY VARCHAR(20),
    AREA VARCHAR(50),
    LOCATION VARCHAR(50),
    ACTIVITY VARCHAR(50),
    NAME VARCHAR(50),
    SEX VARCHAR(50),
    NAMES VARCHAR(50),
    AGE VARCHAR(50),
    INJURY VARCHAR(50),
    FATAL VARCHAR(50),
    FATALS VARCHAR(50),
    TIMES VARCHAR(50),
    SPECIES VARCHAR(50),
    NOTE VARCHAR(50),
    INVESTIGATOR VARCHAR(50)
